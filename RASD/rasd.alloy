abstract sig User {}
sig Student extends User{}
sig Educator extends User{}

abstract sig Status{}
sig Open extends Status{}
sig Closed extends Status{}
sig Completed extends Status{}

sig Team{
    team_leader: one Student,
    team_mates: set Student,
}

sig Battle{
    creator: one Educator,
    teams: set Team,
    status: one Status
}

sig Tournament{
    creator: one Educator,
    allowed_educators: set Educator,
    battles: set Battle,
    status: one Status
}


